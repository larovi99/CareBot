#include <Servo.h>Servo servoMotorShoulder;Servo servoMotorElbow;Servo servoMotorWrist;Servo servoMotorClamp;float length1=20;float length2=20;float clampLength = 15;int pinServoMotorShoulder = 1;int pinServoMotorElbow = 1;int pinServoMotorWrist = 1;int pinServoMotorClamp = 1;float objectDistance;float objectHeight;void setup() {  servoMotorShoulder.attach(pinServoMotorShoulder);  servoMotorElbow.attach(pinServoMotorElbow);  servoMotorWrist.attach(pinServoMotorWrist);  servoMotorClamp.attach(pinServoMotorClamp);}void loop() {  float distance = objectDistance-clampLength;  float h = sqrt(pow(distance,2)+pow(objectHeight,2));    float angle2 = acos((pow(h,2)-pow(length1,2)-pow(length2,2))/(2*length1*length2));
  float angle1 = atan2(objectHeight,distance)-atan2(length2*sin(angle2),length1+length2*cos(angle2));
  angle2 = angle2*57.2958;
  angle1 = angle1*57.2958;  servoMotorClamp.write(180);//fully open  wait(100);  servoMotorShoulder.write(angle1);  servoMotorElbow.write(angle2);  servoMotorWrist.write(-(angle1+angle2));  wait(100);  servoMotorClamp.write(0);    }